{
  "expo": {
    "name": "expense-tracker",
    "slug": "expense-tracker",
    "scheme": "expense-tracker",
    "version": "0.7.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prathamjaiswal.expensetracker"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.prathamjaiswal.expensetracker"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5e45f82b-8f82-4b9b-b729-30a4492b8688"
      }
    }
  }
}
